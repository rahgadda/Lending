  <dmn:decision id="_FA7D87E8-4DF7-4F6D-B578-190AA7EA5ED0" name="DerivedDetails">
    <dmn:extensionElements/>
    <dmn:variable id="_33DF3C88-590F-4300-B9D2-621ADFE5788C" name="DerivedDetails" typeRef="DerivedDetails"/>
    <dmn:informationRequirement id="_BC5FE6AC-A604-4BB2-BC08-44E899BF4D80">
      <dmn:requiredInput href="#_68853ADF-77EA-4433-8BC9-831FBCBC08A5"/>
    </dmn:informationRequirement>
    <dmn:informationRequirement id="_9946DE18-F972-49BB-9D37-1A66DB02FE7E">
      <dmn:requiredDecision href="#_3CC91CF9-970A-49C1-A3FE-158899892637"/>
    </dmn:informationRequirement>
    <dmn:informationRequirement id="_8C878D12-903B-4017-951F-43927D560A30">
      <dmn:requiredInput href="#_150CEEB0-4C9D-41E1-955D-DE3D7B0F7D38"/>
    </dmn:informationRequirement>
    <dmn:context id="_1A40E7DE-D60E-4772-A25C-DC40B40CB24C">
      <dmn:contextEntry>
        <dmn:variable id="_DE93EE62-268C-4ED8-AD39-BF352B6B2605" name="DerivedDetails" typeRef="DerivedDetails"/>
        <dmn:context id="_1FAD0889-BD3C-4E30-A0E6-1EB68E1EB89D">
          <dmn:contextEntry>
            <dmn:variable id="_4D42000B-B668-4ABE-8A87-B6B5D43615E5" name="YearFactor" typeRef="number"/>
            <dmn:literalExpression id="_499769FC-5B30-4E9D-BF5D-D0058CB5DFB8">
              <dmn:text>Lookup.YearFactor</dmn:text>
            </dmn:literalExpression>
          </dmn:contextEntry>
          <dmn:contextEntry>
            <dmn:variable id="_29E22FC2-A968-48FC-AB0F-FEB6F6F10AF8" name="LoanEndDt" typeRef="date"/>
            <dmn:literalExpression id="_60A193CC-2210-4F6A-928A-3806520876E5">
              <dmn:text>date(LoanDetails.FirstPmtDt)+(duration(Lookup.Duration)*(LoanDetails.Term-1))</dmn:text>
            </dmn:literalExpression>
          </dmn:contextEntry>
          <dmn:contextEntry>
            <dmn:variable id="_CCA5CA41-7B66-46E5-A334-B5918BFDB1B2" name="MinPmtAmt" typeRef="number"/>
            <dmn:literalExpression id="_6665AD2B-4E62-4CB3-B0BB-ED67A7D3619C">
              <dmn:text>LoanDetails.AmtFinanced/LoanDetails.Term</dmn:text>
            </dmn:literalExpression>
          </dmn:contextEntry>
          <dmn:contextEntry>
            <dmn:variable id="_46A8CE65-9DDB-4F62-B11D-49941C510CE8" name="MaxPmtAmt" typeRef="number"/>
            <dmn:literalExpression id="_361C7E22-DC0E-43E3-9878-44ABA1176223">
              <dmn:text>MinPmtAmt+(LoanDetails.AmtFinanced*(LoanDetails.Rate/100)*YearFactor)</dmn:text>
            </dmn:literalExpression>
          </dmn:contextEntry>
          <dmn:contextEntry>
            <dmn:variable id="_33DF6A54-EB60-4037-852D-2A0C5F8952CC" name="PmtAmt" typeRef="number"/>
            <dmn:literalExpression id="_2A9BDA81-E324-4FD5-B648-3C2EC8105E40">
              <dmn:text>if ProductDetails.InstallmentType="EIP" then
    (LoanDetails.AmtFinanced + (LoanDetails.AmtFinanced*(LoanDetails.Rate/100)*YearFactor*LoanDetails.Term))/LoanDetails.Term
else
    0</dmn:text>
            </dmn:literalExpression>
          </dmn:contextEntry>
          <dmn:contextEntry>
            <dmn:variable id="_68EC87DD-4FF5-4453-AF16-E4EC6E2E0015" name="TotalPmts" typeRef="number"/>
            <dmn:literalExpression id="_F5EBB265-FD77-4F4B-AE61-5A80123F1CAA">
              <dmn:text>if ProductDetails.InstallmentType="EIP" then
    PmtAmt*LoanDetails.Term
else
    0</dmn:text>
            </dmn:literalExpression>
          </dmn:contextEntry>
          <dmn:contextEntry>
            <dmn:variable id="_31F73BE2-B2E4-4237-A08A-E0B5358C150D" name="TotalInterest" typeRef="number"/>
            <dmn:literalExpression id="_806B1CAA-5113-448A-B62A-6BF75CE7F18F">
              <dmn:text>if ProductDetails.InstallmentType="EIP" then
    TotalPmts-LoanDetails.AmtFinanced
else
    0</dmn:text>
            </dmn:literalExpression>
          </dmn:contextEntry>
        </dmn:context>
      </dmn:contextEntry>
      <dmn:contextEntry>
        <dmn:literalExpression id="_827BCB58-CCBC-4304-AEF9-92F72908EA66">
          <dmn:text>DerivedDetails</dmn:text>
        </dmn:literalExpression>
      </dmn:contextEntry>
    </dmn:context>
  </dmn:decision>
